<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1661997317396049-1</unique-id>
	<command>/usr/bin/alacritty --working-directory %f</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>edit-copy-path</icon>
	<name>Copy Location</name>
	<unique-id>1663762319488760-1</unique-id>
	<command>/usr/bin/xclip -selection clipboard -t &quot;$(file -b --mime-type %f)&quot; &lt; %f</command>
	<description></description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
</actions>
